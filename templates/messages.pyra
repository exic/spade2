} 

function hidediv() {

var divs = document.getElementsByName( 'message' ) ;

for( i=0; i<divs.length; i++ ) {

if( document.getElementById ) 
divs[i].style.visibility="hidden" ; 
else if( document.layers ) 
document.divs[i].visibility = 'hidden' ; 
else 
document.all.divs[i].visibility = 'hidden' ; 
 
} 
}
//--> 
</script>


<title>@!name!@ - SPADE Web Interface</title>
</head>


<!--(if "acc." in name)-->
	<!--(include)-->
	header_platform.pyra 
	<!--(end)-->
<!--(else)-->
	<!--(include)-->
	header.pyra
	<!--(end)-->
<!--(end)-->

	

	<div class="main">
				<h1>@!name!@</h1>
		<div class="content">
			
			<!--(if diagram!=False)-->
			<img src="@!diagram!@" border=0 alt="Sequence Diagram"></img>
			<!--(end)-->
			
			<FORM class="msgForm" NAME="myform" ACTION="showdiv(this.form)" METHOD="GET">
			Enter message ID: <BR>
			<INPUT TYPE="text" NAME="inputbox" VALUE="" SIZE="6"><INPUT TYPE="button" NAME="button" Value="Show" onClick="showdiv(this.form)">
			</FORM>
			
			<ul>
			<!--(for index,mess in messages.items())-->
				<div id="@!index!@" name='message' style="position:absolute; visibility:hidden">
					<li class="xml">
					<tt>@!mess[0]!@: </tt> <a style="text-align:right; margin-right:0px; margin-left:auto;" href="#" onClick="hidediv()">[Close]</a>
						<pre>@!mess[1]!@</pre>
					</li>
				</div>
			<!--(end)-->
			</ul>
		</div>



</div>

<!--(include)-->
footer.pyra
<!--(end)-->