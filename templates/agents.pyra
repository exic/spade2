					<a href="@!awuis[agent.getAID().getName()]!@/admin">@!str(agent.getAID().getName())!@</a></td>
					<!--(end)-->
					<td class="servBodL">
						<!--(if agent.getAID())-->
						@!str(agent.getAID().getAddresses()).strip("[']")!@
						<!--(end)-->
						</td>
